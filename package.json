{
	"name": "simple-dictionary",
	"displayName": "simple-dictionary",
	"description": "Pop up the meaning of the mouse-over word",
	"version": "0.0.1",
	"publisher": "hoddy3190",
	"repository": {
		"type": "git",
		"url": "https://github.com/hoddy3190/simple-dictionary.git"
	},
	"bugs": {
		"url": "https://github.com/hoddy3190/simple-dictionary/issues"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "simple-dictionary",
			"properties": {
				"simple-dictionary.dictPaths": {
					"title": "Path list of dictionaries",
					"type": "string",
					"default": "",
					"description": "If multiple, use a comma as delimited string (e.g. /path/to/dict1,/path/to/dict2). Dictionary file format is tsv, 'index<tab>meaning'."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"prettier": "2.0.5",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
